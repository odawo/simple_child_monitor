    <TextView
        android:id="@+id/output"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="outcome :"
        android:textSize="20sp"
        android:padding="20dp"
        android:layout_marginTop="90dp"/>



</RelativeLayout>
